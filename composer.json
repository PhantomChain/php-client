{
    "name": "phantomchain/client",
    "description": "A simple PHP API client for the PHANTOM Blockchain.",
    "keywords": ["api", "phantom", "blockchain", "crypto", "currency"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "brian@ark.io",
        "homepage": "https://github.com/faustbrian"
    }],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "suggest": {
        "ext-mbstring": "If ext-mbstring is not available you MUST install symfony/polyfill-mbstring"
    },
    "autoload": {
        "psr-4": {
            "PhantomChain\\Client\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhantomChain\\Tests\\Client\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
